{
    "manifest_version": 3,
    "name": "anyTeX",
    "version": "1.0.0",
    "description": "Write, Render and Screenshot TeX quickly in a popup.",
    "permissions": [
        "scripting",
        "activeTab",
        "contextMenus",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "icons": {
        "128": "images/icon.png"
    }
}